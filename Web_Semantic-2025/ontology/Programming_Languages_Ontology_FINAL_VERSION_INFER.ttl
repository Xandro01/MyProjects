@prefix : <http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology> .

<http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology> rdf:type owl:Ontology ;
                                                                                      <http://purl.org/dc/elements/1.1/creator> "Alessandro Scicolone"@en ,
                                                                                                                                "Patric Reineri"@en ;
                                                                                      <http://purl.org/dc/elements/1.1/date> "2024-05-04"@en ;
                                                                                      <http://purl.org/dc/elements/1.1/description> "An ontology describing programming languages, their paradigms, features, and relationships. It captures information about language classifications (e.g., functional, object-oriented), implementation characteristics, and historical development. The ontology aims to support knowledge representation, semantic integration, and reasoning in software engineering and educational contexts."@en ;
                                                                                      <http://purl.org/dc/elements/1.1/title> "Programming Languages Ontology"@en ;
                                                                                      <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#coversRequirements> "What are the elements in this set?"^^xsd:string ,
                                                                                                                                                                                "What is the cardinality (size) of this set?"^^xsd:string ,
                                                                                                                                                                                "What things are contained in this collection (community, collective)? What collections this thing is member of?" ;
                                                                                      <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#extractedFrom> "http://swan.mindinformatics.org/ontologies/1.2/collections.owl"^^xsd:string ,
                                                                                                                                                                           "http://www.ontologydesignpatterns.org/ont/dul/DUL.owl" ;
                                                                                      <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#hasAuthor> "Aldo Gangemi" ,
                                                                                                                                                                       "PaoloCiccarese"^^xsd:string ;
                                                                                      <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#hasConsequences> "Collections and their members can be associated. Time-indexed membership cannot be represented though (you need a situation-based pattern)." ;
                                                                                      <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#hasIntent> "To model sets of things (elements). A Set is a collection that cannot contain duplicate elements. "^^xsd:string ,
                                                                                                                                                                       "To represent domain (not set theory) membership." ;
                                                                                      <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#reengineeredFrom> "http://swan.mindinformatics.org/ontologies/1.2/collections.owl"^^xsd:string ;
                                                                                      <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#relatedCPs> "TimeIndexedMembership" ,
                                                                                                                                                                        "http://www.ontologydesignpatterns.org/cp/owl/bag.owl"^^xsd:string ,
                                                                                                                                                                        "http://www.ontologydesignpatterns.org/cp/owl/list.owl"^^xsd:string ;
                                                                                      <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#scenarios> "My saxophone collection includes a Mark VI tenor, a Balanced Action alto, and a Conn Transitional bari." ;
                                                                                      :license "MIT"@en ;
                                                                                      :vann:prederredNamespaceUri "http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology"@en ;
                                                                                      :vann:preferredNamespacePrefix "pl"@en ;
                                                                                      rdfs:comment "A Set is a collection that cannot contain duplicate elements. A Set is expressed by linking to it directly all the members (elements), multiple identical values of members (elements) will be eliminated because by default they are treated as a set."^^xsd:string ,
                                                                                                   """The collection entity pattern. 
It is extracted from DOLCE-UltraLite by partial clone of elements.""" ;
                                                                                      rdfs:label "Programming Languages Ontology"@en ,
                                                                                                 "collection entity" ,
                                                                                                 "membership" ;
                                                                                      owl:versionInfo "1.0"@en ,
                                                                                                      "1.1 added rdfs:isDefinedBy for all named entities"^^xsd:string ,
                                                                                                      "Created by Aldo Gangemi and Valentina Presutti"^^xsd:string ,
                                                                                                      "Created with TopBraid Composer"^^xsd:string .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
<http://purl.org/dc/elements/1.1/date> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#coversRequirements
<http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#coversRequirements> rdf:type owl:AnnotationProperty ;
                                                                                          rdfs:comment "This annotation property is used for exemplifying possible requirements the content pattern provides a solution for. Requirements are expressed as natural language competency questions." .


###  http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#extractedFrom
<http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#extractedFrom> rdf:type owl:AnnotationProperty ;
                                                                                     rdfs:comment "This annotation property should be assigned with a URI, which points to the possible reference ontology which the annotated pattern was extracted from (i.e. the reference ontology that the ontology elements have been deeply or partially cloned by). The range is not explicit in the definition of the annotation property because it would turn the ontology to OWL Full. E.g. The participation pattern is extracted from the Dolce Ultra Lite ontology, hence the value for this annotation property is http://www.ontologydesignpatterns.org/ont/dul/DUL.owl" .


###  http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#hasAuthor
<http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#hasAuthor> rdf:type owl:AnnotationProperty .


###  http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#hasConsequences
<http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#hasConsequences> rdf:type owl:AnnotationProperty ;
                                                                                       rdfs:comment "This annotation property is used for briefly describing the benefits and/or possible trade-offs when using the CP." .


###  http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#hasIntent
<http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#hasIntent> rdf:type owl:AnnotationProperty ;
                                                                                 rdfs:comment "This annotation property is used in order to describe the intent of the content pattern." .


###  http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#reengineeredFrom
<http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#reengineeredFrom> rdf:type owl:AnnotationProperty ;
                                                                                        rdfs:comment "This annotation property should be assigned with a URI, which points to the concept schema, page, or anything else from which the annotated pattern was reengineered.  If the source does not have any URI e.g., a printed book, this property value should provide information as precise as possible in order to identify the source. This property is alternative to the extractedFrom property because it is used when the pattern come from a concept schema which is not an owl ontology. For example content ontology design patterns, which are reengineered from data model patterns, rdf schemas, etc. should be annotatd with this property. E.g. The basicpersonalfoaf pattern is extracted from the rdf FOAF specification, hence the value for this annotation property is http://xmlns.com/foaf/spec/20071002.rdf" .


###  http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#relatedCPs
<http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#relatedCPs> rdf:type owl:AnnotationProperty ;
                                                                                  rdfs:comment "This annotation property can be used to indicate other CPs (if any) that specialize, generalize, inlcude, or are components of the CP. Furthermore, this field may indicate other CPs that are typically used in conjunction with the described one. Important similarities and differences with other patterns can be also described here." .


###  http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#scenarios
<http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#scenarios> rdf:type owl:AnnotationProperty ;
                                                                                 rdfs:comment "This annotation property is used for describing examples of instantiation of the Content OP. For example, for the part-of Content OP (which represents part-whole relations) a possible scenario is the sentence: \"the brain is part of the human body\". Scenarios are expressed as natural language sentences." .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#license
:license rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#vann:prederredNamespaceUri
:vann:prederredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#vann:preferredNamespacePrefix
:vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#exactMatch
<http://www.w3.org/2004/02/skos/core#exactMatch> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#architectureType
:architectureType rdf:type rdfs:Datatype ;
                  rdfs:comment "Specifies the type of computer architecture, such as x86, ARM, or MIPS, that a system or execution environment is designed to support"@en ;
                  rdfs:label "architectureType"@en ;
                  owl:equivalentClass xsd:string .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#compileTimeTyping
:compileTimeTyping rdf:type rdfs:Datatype ;
                   rdfs:comment "A type system where type checking is performed during the compilation process, before the program is executed."@en ;
                   rdfs:label "compileTimeTyping"@en ;
                   owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                         owl:oneOf [ rdf:type rdf:List ;
                                                     rdf:first "Dynamic" ;
                                                     rdf:rest [ rdf:type rdf:List ;
                                                                rdf:first "Static" ;
                                                                rdf:rest rdf:nil
                                                              ]
                                                   ]
                                       ] .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#esecutionTimeTyping
:esecutionTimeTyping rdf:type rdfs:Datatype ;
                     rdfs:comment "A type system where type checking is performed at runtime, during the execution of the program, rather than at compile time."@en ;
                     rdfs:label "esecutionTimeTyping"@en ;
                     owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                           owl:oneOf [ rdf:type rdf:List ;
                                                       rdf:first "Strong" ;
                                                       rdf:rest [ rdf:type rdf:List ;
                                                                  rdf:first "Weak" ;
                                                                  rdf:rest rdf:nil
                                                                ]
                                                     ]
                                         ] .


###  internal:anonymous-constants
<internal:anonymous-constants> rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> rdf:type owl:ObjectProperty ;
                                                                              owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf> ;
                                                                              rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> ;
                                                                              rdfs:range owl:Thing ;
                                                                              rdfs:comment "A relation between collections and entities, e.g. 'my collection of saxophones includes an old Adolphe Sax original alto' (i.e. my collection has member an Adolphe Sax alto)." ,
                                                                                           "The link to the members of a Set"^^xsd:string ;
                                                                              rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl> ;
                                                                              rdfs:label "has member"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf> rdf:type owl:ObjectProperty ;
                                                                               rdfs:domain owl:Thing ;
                                                                               rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> ;
                                                                               rdfs:comment "inverse of has member"@en ;
                                                                               rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl> ;
                                                                               rdfs:label "is member of"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#IndirecltyDerivationFrom
:IndirecltyDerivationFrom rdf:type owl:ObjectProperty ,
                                   owl:TransitiveProperty ;
                          rdfs:domain :ProgrammingLanguage ;
                          rdfs:range :ProgrammingLanguage ;
                          owl:propertyChainAxiom ( :originatesFrom
                                                   :originatesFrom
                                                 ) ;
                          rdfs:comment "Indicates that a programming language is influenced by or derived from another programming language through one or more intermediate languages."@en ;
                          rdfs:label "indirectlyDerivationFrom"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#allowsDevelopmentOf
:allowsDevelopmentOf rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain :DevelopmentEnvironment ;
                     rdfs:range :ProgrammingLanguageSet ;
                     rdfs:comment "Indicates that a particular development environment enables the creation or development of a set of programming languages."@en ;
                     rdfs:label "allowsDevelopmentOf"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#canTranslateInto
:canTranslateInto rdf:type owl:ObjectProperty ;
                  rdfs:domain :ProgrammingLanguage ;
                  rdfs:range :MachineLanguage ;
                  owl:propertyChainAxiom ( :use
                                           :translatesInto
                                         ) ;
                  rdfs:comment "Indicates that a programming language can be translated into machine language"@en ;
                  rdfs:label "canTranslateInto"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#contains
:contains rdf:type owl:ObjectProperty ;
          owl:inverseOf :partOf ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :DevelopmentEnvironment
                                      :ExecutionEnvironment
                                    )
                      ] ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( :Compiler
                                     :ExecutionEnvironment
                                     :Interpreter
                                   )
                     ] ;
          rdfs:comment "Indicates that a development or execution environment includes one or more components such as a compiler, an interpreter, or another execution environment."@en ;
          rdfs:label "contains"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#createdBy
:createdBy rdf:type owl:ObjectProperty ;
           owl:inverseOf :isCreatorOf ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :DevelopmentEnvironment
                                       :ProgrammingLanguage
                                     )
                       ] ;
           rdfs:range :ProgrammingLanguageCreator ;
           rdfs:comment "Indicates that a development environment or programming language was developed or authored by an agent, such as a person, group, or organization."@en ;
           rdfs:label "createdBy"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#executionRowByRowFrom
:executionRowByRowFrom rdf:type owl:ObjectProperty ;
                       rdfs:domain :Interpreter ;
                       rdfs:range :ProgrammingLanguage ;
                       rdfs:comment "Indicates that an interpreter executes a programming language line by line or statement by statement, rather than compiling the entire program before execution."@en ;
                       rdfs:label "executionRowByRow"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#implements
:implements rdf:type owl:ObjectProperty ;
            rdfs:domain :ProgrammingLanguage ;
            rdfs:range :ProgrammingParadigm ;
            rdfs:comment "Indicates that a programming language supports or follows a specific programming paradigm, such as functional, object-oriented, or procedural."@en ;
            rdfs:label "implements"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#isCreatorOf
:isCreatorOf rdf:type owl:ObjectProperty ,
                      owl:InverseFunctionalProperty ;
             rdfs:domain :ProgrammingLanguageCreator ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( :DevelopmentEnvironment
                                        :ProgrammingLanguage
                                      )
                        ] ;
             rdfs:comment "Indicates that an agent, such as a person or organization, is the creator of a development environment or programming language."@en ;
             rdfs:label "isCreatorOf"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#originatesFrom
:originatesFrom rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :ProgrammingLanguage ;
                rdfs:range :ProgrammingLanguage ;
                rdfs:comment "Indicates that a programming language has its roots or foundational concepts based on another programming language."@en ;
                rdfs:label "originatesFrom"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#partOf
:partOf rdf:type owl:ObjectProperty ;
        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( :Compiler
                                    :ExecutionEnvironment
                                    :Interpreter
                                  )
                    ] ;
        rdfs:range [ rdf:type owl:Class ;
                     owl:unionOf ( :DevelopmentEnvironment
                                   :ExecutionEnvironment
                                 )
                   ] ;
        rdfs:comment "Indicates that an entity, such as a compiler, interpreter, or execution environment, is a component within a larger development or execution environment."@en ;
        rdfs:label "partOf"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#translatableBy
:translatableBy rdf:type owl:ObjectProperty ;
                owl:inverseOf :translatesInto ;
                rdfs:domain :MachineLanguage ;
                rdfs:range :Compiler ;
                rdfs:comment "Indicates that machine language is the output produced by a compiler translating source code written in a programming language."@en ;
                rdfs:label "translatableBy"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#translateFrom
:translateFrom rdf:type owl:ObjectProperty ;
               rdfs:domain :Compiler ;
               rdfs:range :ProgrammingLanguage ;
               rdfs:comment "Indicates that a compiler takes source code written in a specific programming language as input for translation."@en ;
               rdfs:label "translateFrom"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#translatesInto
:translatesInto rdf:type owl:ObjectProperty ;
                rdfs:domain :Compiler ;
                rdfs:range :MachineLanguage ;
                rdfs:comment "Indicates that a compiler converts source code into machine language as part of the compilation process."@en ;
                rdfs:label "translatesInto"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#use
:use rdf:type owl:ObjectProperty ;
     rdfs:domain :ProgrammingLanguage ;
     rdfs:range [ rdf:type owl:Class ;
                  owl:unionOf ( :Compiler
                                :Interpreter
                              )
                ] ;
     rdfs:comment "Indicates that a programming language relies on either a compiler or an interpreter to be executed or translated."@en ;
     rdfs:label "use"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#writeIn
:writeIn rdf:type owl:ObjectProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain <http://schema.org/SoftwareApplication> ;
         rdfs:range :ProgrammingLanguageSet ;
         rdfs:comment "Indicates that a software application is developed using one or more programming languages from a specific programming language set."@en ;
         rdfs:label "writeIn"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/set.owl#size
<http://www.ontologydesignpatterns.org/cp/owl/set.owl#size> rdf:type owl:DatatypeProperty ;
                                                            rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> ;
                                                            rdfs:range xsd:integer ;
                                                            rdfs:comment "size - The number of items belonging to a collection"^^xsd:string ;
                                                            rdfs:label "size"^^xsd:string .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#compatibleWith
:compatibleWith rdf:type owl:DatatypeProperty ;
                rdfs:domain :Compiler ;
                rdfs:range :architectureType ;
                rdfs:comment "Specifies the type of hardware or system architecture that a compiler is compatible with, such as x86, ARM, or MIPS."@en ;
                rdfs:label "compatibleWith"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#executableBy
:executableBy rdf:type owl:DatatypeProperty ;
              rdfs:domain :MachineLanguage ;
              rdfs:range :architectureType ;
              rdfs:comment "Indicates the type of system architecture capable of directly executing the given machine language code."@en ;
              rdfs:label "executableBy"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#hasCompileTyping
:hasCompileTyping rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf :hasTyping ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:domain :ProgrammingLanguage ;
                  rdfs:range :compileTimeTyping ;
                  rdfs:comment "Indicates that the programming language performs type checking during the compilation process, before the code is executed."@en ;
                  rdfs:label "hasCompileTyping"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#hasEsecutionTyping
:hasEsecutionTyping rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf :hasTyping ;
                    rdf:type owl:FunctionalProperty ;
                    rdfs:domain :ProgrammingLanguage ;
                    rdfs:range :esecutionTimeTyping ;
                    rdfs:comment "Indicates that the programming language performs type checking during program execution."@en ;
                    rdfs:label "hasEsecutionTyping"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#hasLogo
:hasLogo rdf:type owl:DatatypeProperty ;
         rdfs:domain :ProgrammingLanguage ;
         rdfs:range xsd:string ;
         rdfs:comment "Refers to a ProgrammingLanguage Logo"@en ;
         rdfs:label "hasLogo"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#hasTyping
:hasTyping rdf:type owl:DatatypeProperty ;
           rdfs:domain :ProgrammingLanguage ;
           rdfs:range [ rdf:type rdfs:Datatype ;
                        owl:unionOf ( :compileTimeTyping
                                      :esecutionTimeTyping
                                    )
                      ] ;
           rdfs:comment "Specifies whether a programming language performs type checking at compile time or at execution time."@en ;
           rdfs:label "hasTyping"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#isDesignedTo
:isDesignedTo rdf:type owl:DatatypeProperty ;
              rdfs:domain :ExecutionEnvironment ;
              rdfs:range :architectureType ;
              rdfs:comment "Indicates the type of hardware or system architecture for which an execution environment is specifically designed or optimized"@en ;
              rdfs:label "isDesignedTo"@en .


#################################################################
#    Classes
#################################################################

###  http://schema.org/SoftwareApplication
<http://schema.org/SoftwareApplication> rdf:type owl:Class ;
                                        rdfs:comment "A software application."@en ;
                                        rdfs:label "schema:SoftwareApplication"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> rdf:type owl:Class ;
                                                                               rdfs:comment "Any container for entities that share one or more common properties. E.g. \"stone objects\", \"the nurses\", \"the Louvre Aegyptian collection\". A collection is not a logical class: a collection is a first-order entity, while a class is a second-order one." ;
                                                                               rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl> ;
                                                                               rdfs:label "Collection"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/set.owl#Set
<http://www.ontologydesignpatterns.org/cp/owl/set.owl#Set> rdf:type owl:Class ;
                                                           rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> ;
                                                           rdfs:comment "Set - A collection that cannot contain duplicate elements."^^xsd:string ;
                                                           rdfs:label "set"^^xsd:string .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#CompiledProgrammingLanguage
:CompiledProgrammingLanguage rdf:type owl:Class ;
                             owl:equivalentClass [ rdf:type owl:Restriction ;
                                                   owl:onProperty :use ;
                                                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                   owl:onClass :Compiler
                                                 ] ;
                             rdfs:subClassOf :ProgrammingLanguage ;
                             rdfs:comment "A type of programming language in which the source code is transformed into machine code through a compiler before execution."@en ;
                             rdfs:label "CompiledProgramming Language"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#Compiler
:Compiler rdf:type owl:Class ;
          rdfs:comment "A software tool that translates source code written in a programming language into machine code, bytecode, or another programming language."@en ;
          rdfs:label "Compiler"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#DeclarativeLanguage
:DeclarativeLanguage rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty :implements ;
                                           owl:hasValue :DeclarativeParadigm
                                         ] ;
                     rdfs:subClassOf :ProgrammingLanguage ;
                     rdfs:comment "A type of programming language in which the programmer specifies what the program should accomplish rather than how it should be done, focusing on logic and expressions."@en ;
                     rdfs:label "DeclarativeLanguage"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#DevelopmentEnvironment
:DevelopmentEnvironment rdf:type owl:Class ;
                        rdfs:comment "A set of tools and resources used to write, test, and debug software, often including a code editor, compiler, debugger, and build automation tools."@en ;
                        rdfs:label "DevelopmentEnvironment"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#ExecutionEnvironment
:ExecutionEnvironment rdf:type owl:Class ;
                      rdfs:comment "The context or platform in which a program runs, including the hardware, operating system, and any supporting software or virtual machines."@en ;
                      rdfs:label "ExecutionEnvironment"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#FunctionalLanguage
:FunctionalLanguage rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty :implements ;
                                          owl:hasValue :FunctionalParadigm
                                        ] ;
                    rdfs:subClassOf :DeclarativeLanguage ,
                                    :Multiparadigms ;
                    rdfs:comment "A programming language that treats computation as the evaluation of mathematical functions and avoids changing state or mutable data."@en ;
                    rdfs:label "FunctionalLanguage"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#HighLevelCompiler
:HighLevelCompiler rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Restriction ;
                                         owl:onProperty :translateFrom ;
                                         owl:someValuesFrom :HighLevelLanguage
                                       ] ;
                   rdfs:subClassOf :Compiler ;
                   rdfs:comment "A software tool that translates programs written in high-level programming languages into lower-level machine code or intermediate representations."@en ;
                   rdfs:label "HighLevelCompiler"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#HighLevelLanguage
:HighLevelLanguage rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Class ;
                                                                owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                                owl:onProperty :use ;
                                                                                owl:someValuesFrom :HighLevelCompiler
                                                                              ]
                                                                              [ rdf:type owl:Restriction ;
                                                                                owl:onProperty :use ;
                                                                                owl:someValuesFrom :Interpreter
                                                                              ]
                                                                            )
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty :implements ;
                                                                owl:someValuesFrom :ProgrammingParadigm
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ;
                   rdfs:subClassOf :ProgrammingLanguage ;
                   rdfs:comment "A programming language that is designed to be easy for humans to read and write, abstracting away the details of the computer's hardware."@en ;
                   rdfs:label "HighLevelLanguage"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#HybridProgrammingLanguage
:HybridProgrammingLanguage rdf:type owl:Class ;
                           owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                        owl:onProperty :use ;
                                                                        owl:someValuesFrom :Compiler
                                                                      ]
                                                                      [ rdf:type owl:Restriction ;
                                                                        owl:onProperty :use ;
                                                                        owl:someValuesFrom :Interpreter
                                                                      ]
                                                                    ) ;
                                                 rdf:type owl:Class
                                               ] ;
                           rdfs:subClassOf :CompiledProgrammingLanguage ,
                                           :InterpretedProgrammingLanguage ,
                                           [ owl:intersectionOf ( :CompiledProgrammingLanguage
                                                                  :InterpretedProgrammingLanguage
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                           rdfs:comment "A programming language that combines both compilation and interpretation, where some parts of the code are compiled and others are interpreted during execution."@en ;
                           rdfs:label "HybridProgrammingLanguage"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#ImperativeLanguage
:ImperativeLanguage rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty :implements ;
                                          owl:hasValue :ImperativeParadigm
                                        ] ;
                    rdfs:subClassOf :ProgrammingLanguage ;
                    rdfs:comment "A programming language that expresses the logic of a computation in terms of a sequence of statements that change the program's state."@en ;
                    rdfs:label "ImperativeLanguage"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#InterpretedProgrammingLanguage
:InterpretedProgrammingLanguage rdf:type owl:Class ;
                                owl:equivalentClass [ rdf:type owl:Restriction ;
                                                      owl:onProperty :use ;
                                                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onClass :Interpreter
                                                    ] ;
                                rdfs:subClassOf :ProgrammingLanguage ;
                                rdfs:comment "A programming language in which most of the instructions are directly executed by an interpreter, without prior compilation into machine code."@en ;
                                rdfs:label "InterpretedProgrammingLanguage"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#Interpreter
:Interpreter rdf:type owl:Class ;
             rdfs:comment "A program that directly executes instructions written in a programming language without requiring them to be compiled into machine code."@en ;
             rdfs:label "Interpreter"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#LogicLanguage
:LogicLanguage rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :implements ;
                                     owl:hasValue :LogicParadigm
                                   ] ;
               rdfs:subClassOf :DeclarativeLanguage ,
                               :Multiparadigms ;
               rdfs:comment "A programming language based on formal logic, where programs are written as sets of logical statements and computation is performed through logical inference."@en ;
               rdfs:label "LogicLanguage"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#LowLevelCompiler
:LowLevelCompiler rdf:type owl:Class ;
                  rdfs:subClassOf :Compiler ;
                  rdfs:comment "A compiler that translates code from low-level programming languages or assembly into machine code, closely interacting with the hardware architecture."@en ;
                  rdfs:label "LowLevelCompiler"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#LowLevelLanguage
:LowLevelLanguage rdf:type owl:Class ;
                  rdfs:subClassOf :ProgrammingLanguage ;
                  rdfs:comment "A programming language that provides little or no abstraction from a computer's instruction set architecture, often including assembly language or machine code."@en ;
                  rdfs:label "LowLevelLanguage"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#MachineLanguage
:MachineLanguage rdf:type owl:Class ;
                 rdfs:subClassOf :LowLevelLanguage ;
                 rdfs:comment "A low-level programming language consisting of binary code that is directly understood and executed by a computer's central processing unit (CPU)."@en ;
                 rdfs:label "MachineLanguage"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#Multiparadigms
:Multiparadigms rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty :implements ;
                                      owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                      owl:onClass :ProgrammingParadigm
                                    ] ;
                rdfs:subClassOf :ProgrammingLanguage ;
                rdfs:comment "A programming language that supports multiple programming paradigms, allowing the programmer to use different styles or approaches (e.g., imperative, object-oriented, functional) within the same language."@en ;
                rdfs:label "Multiparadigms"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#ObjectOrientedLanguage
:ObjectOrientedLanguage rdf:type owl:Class ;
                        owl:equivalentClass [ rdf:type owl:Restriction ;
                                              owl:onProperty :implements ;
                                              owl:hasValue :ObjectOrientedParadigm
                                            ] ;
                        rdfs:subClassOf :ImperativeLanguage ,
                                        :Multiparadigms ;
                        rdfs:comment "A programming language that is based on the concept of 'objects,' which are instances of classes, and uses techniques such as inheritance, polymorphism, and encapsulation to structure software."@en ;
                        rdfs:label "Object-Oriented Language"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#ProceduralLanguage
:ProceduralLanguage rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty :implements ;
                                          owl:hasValue :ProceduralParadigm
                                        ] ;
                    rdfs:subClassOf :ImperativeLanguage ,
                                    :Multiparadigms ;
                    rdfs:comment "A programming language that follows a set of steps or procedures to execute a task, focusing on the sequence of actions to be performed."@en ;
                    rdfs:label "ProceduralLanguage"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#ProgrammingLanguage
:ProgrammingLanguage rdf:type owl:Class ;
                     owl:disjointUnionOf ( :HighLevelLanguage
                                           :LowLevelLanguage
                                         ) ;
                     rdfs:comment "A formal language comprising a set of instructions used to produce various kinds of output, primarily for creating software applications."@en ;
                     rdfs:label "ProgrammingLanguage"@en ;
                     <http://www.w3.org/2004/02/skos/core#exactMatch> <http://schema.org/ComputerLanguage> .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#ProgrammingLanguageCreator
:ProgrammingLanguageCreator rdf:type owl:Class ;
                            rdfs:subClassOf <http://xmlns.com/foaf/0.1/Agent> ;
                            rdfs:comment "An individual or group responsible for designing, developing, and maintaining a programming language."@en ;
                            rdfs:label "ProgrammingLanguageCreator"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#ProgrammingLanguageSet
:ProgrammingLanguageSet rdf:type owl:Class ;
                        rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/set.owl#Set> ;
                        rdfs:comment "A collection or group of programming languages, often categorized by purpose, paradigm, or domain of use"@en ;
                        rdfs:label "ProgrammingLanguageSet"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#ProgrammingParadigm
:ProgrammingParadigm rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Class ;
                                           owl:oneOf ( :DeclarativeParadigm
                                                       :EventOrientedParadigm
                                                       :FunctionalParadigm
                                                       :ImperativeParadigm
                                                       :LogicParadigm
                                                       :ObjectOrientedParadigm
                                                       :ProceduralParadigm
                                                       :StructuralParadigm
                                                     )
                                         ] ;
                     rdfs:comment "A style or approach to programming that influences how solutions are designed and implemented."@en ;
                     rdfs:label "ProgrammingParadigm"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#SemiCompiledProgrammingLanguage
:SemiCompiledProgrammingLanguage rdf:type owl:Class ;
                                 rdfs:subClassOf :HybridProgrammingLanguage ;
                                 rdfs:comment "A programming language that is first compiled into an intermediate code (such as bytecode) and then interpreted or executed by a runtime environment."@en ;
                                 rdfs:label "Semi-compiledProgrammingLanguage"@en .


###  http://www.semanticweb.org/patric/ontologies/2025/3/Programming_Languages_Ontology#StructuralLanguage
:StructuralLanguage rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty :implements ;
                                          owl:hasValue :StructuralParadigm
                                        ] ;
                    rdfs:subClassOf :ImperativeLanguage ,
                                    :Multiparadigms ;
                    rdfs:comment "A programming language that emphasizes the logical structure of a program, typically by organizing code into modules or blocks to improve readability and maintainability."@en ;
                    rdfs:label "StructuralLanguage"@en .


###  http://xmlns.com/foaf/0.1/Agent
<http://xmlns.com/foaf/0.1/Agent> rdf:type owl:Class ;
                                  rdfs:comment "An agent (eg. person, group, software or physical artifact)."@en ;
                                  rdfs:label "foaf:Agent"@en .


